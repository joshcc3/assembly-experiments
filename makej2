nasm -f elf64 -i . -g -F dwarf f_lang.s -o out/f_lang.o
nasm -f elf64 -i . -g -F dwarf syscalls.s -o out/syscalls.o 
nasm -f elf64 -i . -g -F dwarf assert.s -o out/assert.o 
nasm -f elf64 -i . -g -F dwarf threads.s -o out/threads.o 
nasm -f elf64 -i . -g -F dwarf test_syscalls.s -o out/test_syscalls.o 
nasm -f elf64 -i . -g -F dwarf lock.s -o out/lock.o 
ld out/*.o -o test_syscalls
